{
  "name": "dashboard",
  "version": "1.0.0",
  "description": "Dashboard that takes smart watch data and displays it with other modules",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haylee430/Project_3.git"
  },
  "keywords": [
    "Dashboard",
    "Data",
    "Smart",
    "Watch"
  ],
  "author": "Ashley Wegwerth, Haylee Thomas-Kuhlmann, Ka Vang, Mai Xiong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/haylee430/Project_3/issues"
  },
  "homepage": "https://github.com/haylee430/Project_3#readme",
  "dependencies": {
    "antd": "^4.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^4.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "jwt-decode": "^2.2.0",
    "moment": "^2.24.0",
    "mongoose": "^5.9.1",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
